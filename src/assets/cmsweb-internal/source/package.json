{
  "name": "cmsweb-internal",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve --port 3001",
    "start:stage": "vue-cli-service serve --port 3001 --mode stage",
    "start:qc": "vue-cli-service serve --port 3001 --mode qc",
    "start:online": "vue-cli-service serve --port 3001 --mode online",
    "build:stage": "yarn run convert && vue-cli-service build --mode stage",
    "build:qc": "yarn run convert && vue-cli-service build --mode qc",
    "build:online": "yarn run convert && vue-cli-service build --mode online",
    "lint": "vue-cli-service lint",
    "node": "node server.js",
    "convert": "node convert-excel",
    "lazy": "yarn run build:stage && yarn run start:stage"
